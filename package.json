    "nuxt": "^4.0.3",
    "nuxt-security": "^2.3.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@dethdkn/eslint-config": "beta",
