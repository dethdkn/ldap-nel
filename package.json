{
  "name": "ldap-nel",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "description": "üîê Modern web interface for LDAP administration",
  "author": {
    "name": "Gabriel 'DethDKN' Rosa",
    "email": "gabriel@rosa.dev.br",
    "url": "https://rosa.dev.br"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/dethdkn",
  "homepage": "https://ldap-nel.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dethdkn/ldap-nel.git"
  },
  "bugs": "https://github.com/dethdkn/ldap-nel/issues",
  "keywords": ["ldap"],
  "scripts": {
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "prerelease": "pnpm lint && pnpm typecheck",
    "docker:build": "docker build --platform=linux/amd64 -t dethdkn/ldap-nel:latest -t dethdkn/ldap-nel:$(node -p \"require('./package.json').version\") .",
    "release": "pnpm prerelease && bumpp && pnpm docker:build && docker push dethdkn/ldap-nel:latest && docker push dethdkn/ldap-nel:$(node -p \"require('./package.json').version\")"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.63",
